. functions/math_functions

is_int 1 && echo 1 ist eine ganze Zahl
is_int 1 ||  echo 1 ist keine ganze Zahl
is_int -99999 && echo -99999 ist eine ganze Zahl
is_int -99999 && echo -99999 ist keine ganze Zahl
is_int TTTT  && echo TTTT ist eine ganze Zahl
is_int TTTT  && echo TTTT ist keine ganze Zahl

ergebnis=$(summe_int 1 2 3 4 1000)
echo ergebnis: $ergebnis

ergebnis=`summe_int 1 T 3`
echo ergebnis: $ergebnis
